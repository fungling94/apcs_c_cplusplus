#include <stdio.h>
#include<iostream>

using namespace std;
int main(void)
{
   int a,b;
   printf("first number: \n");
   scanf("%d",&a);
   printf("second number:\n");
   scanf("%d",&b);
   if(a>b){
        printf("a>b");
   }else if (a<b){
       printf("a<b");
   }else{
       printf("a=b");
   }
    printf("\n Hello world");
   return 0;
}
