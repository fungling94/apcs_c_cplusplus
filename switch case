#include <stdio.h>
#include<iostream>
using namespace std;

int main(void)
{
   int i;
   printf("number: \n");
   scanf("%d",&i);
       switch (i){
        case 1:
        printf("monday");
        break;
        case 2:
        printf("tuesday");
        default:
        printf("error");
       }
       }
